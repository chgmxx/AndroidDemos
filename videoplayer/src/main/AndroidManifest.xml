<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.iflab.ibistubydreamfactory"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.PlayerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="org.iflab.ibistubydreamfactory.activities.NormalPlayerActivity"
            android:configChanges="screenSize|orientation"
            android:label="MDMultiDemoActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name="org.iflab.ibistubydreamfactory.activities.VRVideoPlayerActivity"
            android:configChanges="screenSize|orientation"
            android:label="MDMultiDemoActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name="org.iflab.ibistubydreamfactory.activities.VRImagePlayerActivity"
            android:configChanges="screenSize|orientation"
            android:label="MDMultiDemoActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>
